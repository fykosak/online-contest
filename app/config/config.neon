tracy:
    email: webmaster@fykos.cz

php:
    date.timezone: Europe/Prague

application:
    errorPresenter: Frontend:Error
    mapping:
        *: App\*Module\Presenters\*Presenter

session:
    expiration: 14 days

services:
    - App\Model\Authentication\TeamAuthenticator
    - App\Model\Authentication\OrgAuthenticator(%orgs%)
    - App\Model\Authentication\CronAuthenticator(%cron.key%)
    #Nette\Security\IAuthorizator: SimpleAuthorizator
    router: App\FrontendModule\FrontendModule::createRouter
    - App\Model\Interlos
    - App\Model\AnswersModel
    - App\Model\ChatModel
    - App\Model\CompetitorsModel
    - App\Model\GroupsModel
    - App\Model\NotificationModel
    - App\Model\PeriodModel
    - App\Model\SchoolsModel
    - App\Model\ScoreModel
    - App\Model\TasksModel
    - App\Model\TeamsModel
    - App\Model\YearsModel
    nette.userStorage:
        setup:
            - setNamespace("olfyziklani")

extensions:
    dibi: Dibi\Bridges\Nette\DibiExtension22

includes:
        - acl.neon
